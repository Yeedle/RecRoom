            </top>
            <center>
                <VBox>
                <HBox BorderPane.alignment="CENTER" alignment="CENTER">
                    <Button alignment="CENTER" text="Start A New Game">

                    </Button>
                </HBox>
                    <TableView>
                    </TableView>
                </VBox>
            </center>
        </BorderPane>
    </children>
    <stylesheets>
        <URL value="@../css/global.css" />
        <URL value="@../css/gameIntro.css" />
